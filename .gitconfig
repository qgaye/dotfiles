[user]
	name = qgaye
	email = qgaye0601@outlook.com
[core]
	quotepath = false
	excludesfile = /Users/qgaye/.gitignore_global
	autocrlf = false
	pager = diff-so-fancy | less --tabs=4 -RFX
[alias]
    st = status
    co = checkout
    ci = commit
    cia = commit --amend
    br = branch
    ps = push
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    type = cat-file -t
    dump = cat-file -p
    fr = "!f() { git fetch $1 $2 && git rebase $1/$2; }; f"
[commit]
	template = /Users/qgaye/.stCommitMsg
